 / {
	soc {
		pinctrl: pinctrl@0xA6003000 {
			compatible = "hobot,x2-pinctrl";
			reg = <0 0xA6003000 0 0x100>;
			interrupt-parent = <&gic>;
			interrupts = <0 45 4>;
			gpioirq-bank-cfg = <12 0 0 0>;/*specific the gpio of each irq banck, 0 is invalid*/
			status = "disabled";
			i2c0_func: i2c0_func {
				pinctrl-x2,pins-muxs = <
					9	0 /*i2c SCL*/
					10	0 /*i2c SDA*/
				>;
			};
			i2s0_func: i2s0_func {
				pinctrl-x2,pins-muxs = <
					88 0
					89 0
					90 0
					91 0
				>;
			};
			i2s1_func: i2s1_func {
				pinctrl-x2,pins-muxs = <
					5 1
					61 2
					62 2
					63 2
				>;
			};
			btin_func:btin_func {
				pinctrl-x2,pins-muxs = <
					61	0
					62	0
					63	0
					64	0
					65	0
					66	0
					67	0
					68	0
					69	0
					70	0
					71	0
					72	0
					73	0
					74	0
					75	0
					76	0
					77	0
				>;
			};
			btout_func: btout_func {
				pinctrl-x2,pins-muxs = <
					93	0
					94	0
					95	0
					96	0
					97	0
					98	0
					99	0
					100	0
					101	0
					102	0
					103	0
					104	0
					105	0
					106	0
					107	0
					108	0
					109	0
				>;
			};
			dvpin_func: dvpin_func {
				pinctrl-x2,pins-muxs = <
					61	1
