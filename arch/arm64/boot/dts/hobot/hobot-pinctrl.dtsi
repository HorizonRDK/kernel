					95	1
					96	1
					97	1
					98	1
					99	1
					100	1
					101	1
					102	1
					103	1
					104	1
					105	1
					106	1
					107	1
				>;
			};
			bifsd_func:bifsd_func {
				pinctrl-x2,pins-muxs = <
					15	0
					16	0
					17	0
					18	0
					19	0
					20	0
					21	0
					22	0
					23	0
					24	0
					25	0
				>;
			};
			sdio0_func:sdio0_func {
				pinctrl-x2,pins-muxs = <
					46	0
					47	0
					48	0
					49	0
					50	0
					51	0
					52	0
					53	0
					54	0
