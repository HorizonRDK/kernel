&i2c0 {
	status = "okay";
};
&i2c1 {
	status = "okay";
};


&pinctrl {
	status = "okay";
	gpioirq-bank-cfg = <85 0 0 0>;
};

&uart0 {
	status = "okay";
	clocks = <&uart0_mclk>;
	clock-names = "uart_mclk";
};
/*
&ips {
	status = "okay";
};
*/
&isp {
	status = "okay";
};
