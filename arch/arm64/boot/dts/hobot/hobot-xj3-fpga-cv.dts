&dma {
	status = "okay";
};

&dmatest {
	status = "disable";
};

&rtc {
	status = "okay";
};

&pwm_0 {
	status = "disabled";
};

&spi0 {
	status = "okay";
};

&ethernet {
	status = "okay";

	/* local-mac-address = [ 00 11 22 33 44 55 ]; */
	phy-handle = <&phy1>;
	phy-mode = "rgmii-id";
	mdio {
		#address-cells = <0x1>;
		#size-cells = <0x0>;
		phy1: phy@3 {
			compatible = "marvell,88E1111","ethernet-phy-ieee802.3-c22";
			reg = <0x3>;
		};
	};
};

&cnn0 {
	status = "okay";
};

&cnn1 {
	status = "okay";
};

&iar {
        status = "okay";
};

&bifspi {
	status = "okay";
};

&bifsd {
	status = "okay";
	/*cd-gpio = <77>;*/
};

&bifbase {
	bifbase_irq_pin = <85>;
	bifbase_tri_pin = <84>;
};

&nor_flash {
	status = "okay";
	#address-cells = <1>;
	#size-cells = <0>;

	flash@0 {
		compatible = "gd256q256c";
		#address-cells = <1>;
		#size-cells = <1>;
		reg = <0x0>;
		spi-tx-bus-width = <4>;
		spi-rx-bus-width = <4>;
		spi-max-frequency = <20000000>; /* For FPGA max clk is 20Mhz */

		partition@qspi-rom {
			label = "qspi-rom";
			reg = <0x000000 0x200000>;
		};

		partition@qspi-uboot {
			label = "qspi-uboot";
			reg = <0x200000 0x200000>;
		};
	};
};

&sdio0 {
	status = "okay";
};

&sdio1 {
	status = "disabled";
};

&watchdog {
	status = "okay";
};

&i2s0 {
	status = "okay";
	clocks = <&i2s0_mclk>, <&i2s0_bclk>;
	clock-names = "i2s_mclk", "i2s_bclk";
};

&i2s1 {
	status = "okay";
	clocks = <&i2s1_mclk>, <&i2s1_bclk>;
	clock-names = "i2s_mclk", "i2s_bclk";
};

&jpu {
	status = "okay";
};

&vpu {
	status = "okay";
};

&timer0 {
	timer-freq = <20000000>;
};

&timer1 {
	timer-freq = <20000000>;
};

&timer2 {
	timer-freq = <20000000>;
};

&arch_timer {
	clock-frequency = <20000000>;
};
